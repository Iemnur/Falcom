[{"rva": "19F108", "text": "Chinese_People's Republic of China.936"}, {"rva": "18F9A4", "text": "背後をとられた！"}, {"rva": "18F9CC", "text": "先制攻撃！"}, {"rva": "18F9F0", "text": "   移  動   "}, {"rva": "18FA08", "text": "　 攻　撃 　"}, {"rva": "18FA50", "text": "連携させるキャラが選択されていません。"}, {"rva": "18FA80", "text": "クラフト"}, {"rva": "18FA98", "text": "チェインクラフト"}, {"rva": "18FADC", "text": "パーティは全滅しました···"}, {"rva": "18FB0C", "text": "敵に逃げられました···"}, {"rva": "18FB38", "text": "パーティは退却しました···"}, {"rva": "18FB60", "text": "   アーツ   "}, {"rva": "18FB78", "text": "  クラフト  "}, {"rva": "18FB90", "text": "   道  具   "}, {"rva": "18FBBC", "text": "%c%c%s%c%cをＳブレイク用に設定しますか？"}, {"rva": "18FC48", "text": "%c%c%s%c%cをＳブレイク用に設定しました。\n\t\t\t\t\t\t\t\t\tキャンプ画面の[%c%c%s%c%c]で変更出来ます。"}, {"rva": "193044", "text": "全てのアイテムを表示します。"}, {"rva": "193078", "text": "武器を表示します。"}, {"rva": "19309C", "text": "防具とアクセサリーを表示します。"}, {"rva": "1930D4", "text": "薬と携帯食糧を表示します。"}, {"rva": "193104", "text": "クオーツを表示します。"}, {"rva": "193130", "text": "イベントアイテムを表示します。"}, {"rva": "193168", "text": "食材を表示します。"}, {"rva": "19318C", "text": "書物を表示します。"}, {"rva": "1931BC", "text": "釣りに関係するアイテムを表示します。"}, {"rva": "1931F8", "text": "装備をはずします。"}, {"rva": "19320C", "text": "地"}, {"rva": "193210", "text": "水"}, {"rva": "193214", "text": "火"}, {"rva": "193218", "text": "風"}, {"rva": "19321C", "text": "時"}, {"rva": "193220", "text": "空"}, {"rva": "193224", "text": "幻"}, {"rva": "193284", "text": "【属性:#%dI%s 属性値:"}, {"rva": "1932AC", "text": "】\n"}, {"rva": "1932BC", "text": "なし】\n"}, {"rva": "1932C4", "text": " 時"}, {"rva": "1932D4", "text": "【#%dI%s: "}, {"rva": "1932E4", "text": "】"}, {"rva": "193304", "text": "クオーツを装着できます。"}, {"rva": "19334C", "text": "武器を装備できます。"}, {"rva": "193380", "text": "衣服を装備できます。"}, {"rva": "1933B4", "text": "靴を装備できます。"}, {"rva": "1933E4", "text": "装飾品を装備できます。"}, {"rva": "193514", "text": "◆━━━━━━━━━━━━━━━━━━━━━◆"}, {"rva": "193568", "text": "◆━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━◆"}, {"rva": "1935D4", "text": " を手に入れた"}, {"rva": "193614", "text": "%sは "}, {"rva": "19361C", "text": " を習得した。"}, {"rva": "193638", "text": "◆━━━━━━━◆"}, {"rva": "193948", "text": "%sはアーツを使う準備をしている。\n"}, {"rva": "193978", "text": "%sは逃げ出した。"}, {"rva": "193D58", "text": "　%s　"}, {"rva": "193D94", "text": "アイテム「%s」を壊された！\n"}, {"rva": "193F44", "text": "ＡＴ(Action Time)バー:"}, {"rva": "193F60", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\t行動順を表すバーです。\n\t\t\t\t\t\t\t\t\t上から順に行動が回ってきます。"}, {"rva": "19404C", "text": "攻撃："}, {"rva": "194058", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\t敵を攻撃します。\n\t\t\t\t\t\t\t\t\t敵のいない場所をクリックすると\n\t\t\t\t\t\t\t\t\tそこまで移動することができます（マウス操作時）。"}, {"rva": "194178", "text": "#0C表示される範囲は移動攻撃の届く距離です。\n\t\t\t\t\t\t\t\t\tこの範囲内にいる目標を選択すると\n\t\t\t\t\t\t\t\t\t自動的に移動して攻撃をしかけます。"}, {"rva": "1942F0", "text": "#0Cカーソルを重ねたときに\n\t\t\t\t\t\t\t\t\t　　が表示される目標は攻撃範囲外です。\n\t\t\t\t\t\t\t\t\t選択すると最も接近できる地点まで\n\t\t\t\t\t\t\t\t\t移動しますが、攻撃はできません。"}, {"rva": "194468", "text": "ＡＴボーナス(行動順ボーナス)："}, {"rva": "194488", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\t　行動順についたボーナスを示すアイコンです。\n\t\t\t\t\t\t\t\t\t　これらのアイコンがあるときに\n\t\t\t\t\t\t\t\t\t　行動順が回ってくると各種のボーナスがつきます。"}, {"rva": "194580", "text": "#0C   ：ＨＰ回復、   ：セピスボーナスなど\n\t\t\t\t\t\t\t\t\tアイコンごとに特定の効果があります。"}, {"rva": "194688", "text": "アーツ："}, {"rva": "194698", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\t武器による攻撃が当たりにくい敵や\n\t\t\t\t\t\t\t\t\t攻撃の効果が薄い敵にはアーツが有効です。"}, {"rva": "194768", "text": "アーツ"}, {"rva": "194770", "text": "%c%c%s%c%cは発動までに時間（駆動時間）が掛かります。\n\t\t\t\t\t\t\t\t\tまた、使用すると%c%c%s%c%cを消費します。"}, {"rva": "194850", "text": "属性"}, {"rva": "194858", "text": "%c%c%s%c%cには%c%c%s%c%cがあります。\n\t\t\t\t\t\t\t\t\t相手に有効な属性を見極めて使いましょう。"}, {"rva": "19494C", "text": "クラフト："}, {"rva": "194958", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\tクラフトはキャラクターごとに固有の技です。\n\t\t\t\t\t\t\t\t\t攻撃だけでなく、様々な効果のものがあります。"}, {"rva": "194A60", "text": "%c%c%s%c%cを使うと%c%c%s%c%cを消費します。\n\t\t\t\t\t\t\t\t\t%c%c%s%c%cは攻撃したり、ダメージを受けたりすることで\n\t\t\t\t\t\t\t\t\t戦闘中に少しずつ溜まっていきます。"}, {"rva": "194BC4", "text": "Ｓブレイク"}, {"rva": "194BD0", "text": "Ｓクラフト"}, {"rva": "194BDC", "text": "Ｓブレイク："}, {"rva": "194BF0", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\t%c%c%s%c%cが100以上溜まると、ＡＴバー（行動順）を\n\t\t\t\t\t\t\t\t\t無視して%c%c%s%c%cを使用することが出来ます。\n\t\t\t\t\t\t\t\t\tこれが%c%c%s%c%cです。"}, {"rva": "194D28", "text": "%c%c%s%c%cとして発動する%c%c%s%c%cの変更は\n\t\t\t\t\t\t\t\t\tキャンプ[Tactics]内の［Ｓブレイク登録］で行います。"}, {"rva": "194E38", "text": "ブレイクボタン"}, {"rva": "194E48", "text": "%c%c%s%c%c　　　をクリックすると%c%c%s%c%cが発動します。\n\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\t※ブレイクボタンが　　　になっている間は\n\t\t\t\t\t\t\t\t\t  Ｓブレイクを発動することはできません。"}, {"rva": "194FC8", "text": "#0Cでは、実際に%c%c%s%c%c　　　をクリックし\n\t\t\t\t\t\t\t\t\t%c%c%s%c%cを発動してみましょう。\n\t\t\t\t\t\t\t\t\t※キーボード操作の場合は\n\t\t\t\t\t\t\t\t\t　パーティメンバーに対応した[1][2][3][4]キーを押すか\n\t\t\t\t\t\t\t\t\t　矢印キーの左右[←][→]で選択します。"}, {"rva": "1950C4", "text": "#0CNPCを守れ！"}, {"rva": "195100", "text": "#0CNPCのHPが0になるとゲームオーバーになります。"}, {"rva": "1951BC", "text": "チェインクラフト："}, {"rva": "1951D0", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\tクラフトの中の%c%c%s%c%cは\n\t\t\t\t\t\t\t\t\t複数のキャラが連続して攻撃を仕掛ける技です。"}, {"rva": "1952B0", "text": "%c%c%s%c%c\n\t\t\t\t\t\t\t\t\tチェインクラフトは、参加するキャラを選択した後に\n\t\t\t\t\t\t\t\t\t攻撃対象を選択します。"}, {"rva": "1953B8", "text": "#0C参加するには一定以上のＣＰが必要です。\n\t\t\t\t\t\t\t\t\t必要ＣＰと参加可能人数は、左上の　　 に表示されます。"}, {"rva": "1954C0", "text": "#0C参加する人数が増えるほど攻撃範囲が広がり\n\t\t\t\t\t\t\t\t\t連続攻撃の後ろのキャラほど攻撃力が上がっていきます。"}, {"rva": "195568", "text": "◆━━━━━━━━━━━━━━◆"}, {"rva": "1955B4", "text": "なし"}, {"rva": "1955DC", "text": "属性攻撃有効率(%):"}, {"rva": "195774", "text": "リトライ"}, {"rva": "195780", "text": "タイトルに戻る"}, {"rva": "195790", "text": "先に進める"}, {"rva": "1957CC", "text": "ミラ·セピス"}, {"rva": "1957E8", "text": "引き継ぐ"}, {"rva": "195808", "text": "引き継がない"}, {"rva": "195818", "text": "アイテム"}, {"rva": "195830", "text": "装備品"}, {"rva": "195844", "text": "手帳"}, {"rva": "195854", "text": "ステータス"}, {"rva": "195860", "text": "◆━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━◆"}, {"rva": "1958AC", "text": "決定"}, {"rva": "1958D0", "text": "ミラとセピスとメダルを引き継ぎます。"}, {"rva": "19591C", "text": "各種アイテム(回復、食材、本、釣具など)を引き継ぎます。"}, {"rva": "19596C", "text": " and quartz (~10 per).\n※Quest-related items not included."}, {"rva": "1959A8", "text": "装備品とクオーツを引き継ぎます(各アイテム10個まで)。\n※クエスト関連アイテムを除きます。"}, {"rva": "195A38", "text": "レシピ手帳、魔獣手帳、つり手帳、\n王国地図の内容を引き継ぎます。"}, {"rva": "195AA4", "text": "レベルとスロットの強化状況を引き継ぎます。"}, {"rva": "195C38", "text": "敵の強さを変更できます。\n【ノーマル】標準的なバランスです。適度な刺激を楽しみたい方に。"}, {"rva": "195C90", "text": "敵の強さを変更できます。\n【ハード】やや厳しいバランスです。ノーマルでは物足りない方に。"}, {"rva": "195CE8", "text": "敵の強さを変更できます。\n【ナイトメア】まさに悪夢。各種データを引き継がないと厳しいバランスです。"}, {"rva": "195D50", "text": "敵の強さを変更できます。\n【イージー】サクサク進める易しいバランスです。時間のない方や初心者向け。"}, {"rva": "195DD8", "text": "以上の設定でゲームを開始します。"}, {"rva": "195EC0", "text": "これ以上持てません。"}, {"rva": "195EF0", "text": "%c#2C【%s】#5Cを、%d個手に入れた。"}, {"rva": "195F34", "text": "%c#2C【%s】#5Cを料理して食べた。"}, {"rva": "195F80", "text": "%c#5C%sはＨＰが%d#5Cになり、ＣＰは%d#5Cになった。"}, {"rva": "195FD0", "text": "%c#5C%sはＣＰが%d#5C回復した。"}, {"rva": "196040", "text": "%c#2C【%s】#5Cを料理して食べた。\n全員のＨＰが%d#5C、ＣＰが%d#5C回復した。"}, {"rva": "1960CC", "text": "%c#2C【%s】#5Cを料理して食べた。\n全員のＨＰが%d#5C回復した。"}, {"rva": "19615C", "text": "#2C【%s】#0Cを、何個作りますか？"}, {"rva": "196194", "text": "#2C【%s】#0Cを作りますか？"}, {"rva": "1961B8", "text": "――――――――――――"}, {"rva": "1964C4", "text": "は、ファイルバージョンが古く読み込めません。"}, {"rva": "196DB8", "text": "点の座標値\t\t\t\t\t\t\n; v1\tTexture\t\t\t\t\t\t\t\t\n; c0\t定数 {1, 0,  0.5, 0}\t\t\t\t\n; c2\t定数 {1/TextureW, 1/TextureH,0,0}\t\ndcl_position v0\ndcl_normal v1\nmov\toPos, v0\t\t\t\t\t\t\t\n; テクセルを１つ分ずらすには、テクスチャサイズの逆数だけずらす\nadd\tr0.x, v1.x, -c2.x\t\t\t\t\t\nadd\tr0.y, v1.y, -c2.y\t\t\t\t\t\nmov\toT0.xy,r0.xy\t\t\t\t\t\t\nadd\tr0.x, v1.x,  c2.x\t\t\t\t\t\nadd\tr0.y, v1.y, -c2.y\t\t\t\t\t\nmov\toT1.xy,r0.xy\t\t\t\t\t\t\nadd\tr0.x, v1.x, -c2.x\t\t\t\t\t\nadd\tr0.y, v1.y,  c2.y\t\t\t\t\t\nmov\toT2.xy,r0.xy\t\t\t\t\t\t\nadd\tr0.x, v1.x,  c2.x\t\t\t\t\t\nadd\tr0.y, v1.y,  c2.y\t\t\t\t\t\nmov\toT3.xy,r0.xy\t\t\t\t\t\t\n"}, {"rva": "196FE8", "text": " ステージ０のテクスチャの色\nmul\t\tr1,\t\t\tv0,\t\t\tt0\t\t\t; r1 = 頂点色 * テクスチャ色\ndp3\t\tr0,\t\t\tr1,\t\t\tc[0]\t\t; ピクセル色 = r1 * 定数\nmov\t\tr0.a,\t\tr1.a;\n"}, {"rva": "197098", "text": " ステージ０のテクスチャの色\ndp3\t\tr0,\t\t\tt0,\t\t\tc0\t\t; ピクセルの光度を求める\nmov\t\tr1,\t\t\tr0\ncnd\t\tr0,\t\t\tr0.a,\t\tr0, c1.a    ; d = ( r0.a >0.5 ? s1 : s2 )\nsub\t\tr0,\t\t\tr0,\t\t\tc1\n\t; -0.5add\t\tr0,\t\t\tr0,\t\t\tr0\nadd\t\tr0,\t\t\tr0,\t\t\tr0\ncnd\t\tr0,\t\t\tr0.a,\t\tr0, c1.a    ; d = ( r0.a >0.5 ? s1 : s2 )\nsub\t\tr0,\t\t\tr0,\t\t\tc1\n\t; -0.5"}, {"rva": "197B68", "text": "セーブに失敗しました。セーブフォルダを確認して下さい。\nセーブファイルのパスは以下の通り\n%s"}, {"rva": "197D90", "text": " ？？？？？？？？？"}, {"rva": "197DAC", "text": "最大記録"}, {"rva": "197DC4", "text": "釣れた回数"}, {"rva": "197DDC", "text": "釣れたエサ"}, {"rva": "197E00", "text": "最近釣れた場所"}, {"rva": "197E20", "text": "リジュ"}, {"rva": "197FD8", "text": "赤の"}, {"rva": "197FE8", "text": "黒の"}, {"rva": "198008", "text": "%s%sです。%c"}, {"rva": "19802C", "text": "残念でした。%c"}, {"rva": "198050", "text": "お見事です。%c"}, {"rva": "19807C", "text": "続けますか？"}, {"rva": "198094", "text": "終了しますか？"}, {"rva": "1980B8", "text": "またお越しください。%c"}, {"rva": "19812C", "text": "ディーラー"}, {"rva": "198138", "text": "始める"}, {"rva": "198140", "text": "終わる"}, {"rva": "198148", "text": "説明を聞く"}, {"rva": "1981E8", "text": "メダルをお持ちでないようですね。\n交換所でメダルとミラを交換してください。%c"}, {"rva": "198254", "text": "ルーレット台へようこそ。"}, {"rva": "198270", "text": "はい"}, {"rva": "198278", "text": "いいえ"}, {"rva": "1982A0", "text": "０,００,１～３６までの数字の書いてあるルーレットに玉を転がして\n止まる場所の数字を予想するギャンブルです。"}, {"rva": "198310", "text": "１つの数字に賭ける時は、その数字の真上にメダルを置きます。\nこの場合、倍率は３６倍です。"}, {"rva": "198368", "text": "２つの数字に賭けます。\nこの場合、２０と２３に賭けています。倍率は１８倍です。"}, {"rva": "1983B8", "text": "３つの数字（ストリート）に賭けます。\nこの場合、１３,１４,１５に賭けています。倍率は１２倍です。"}, {"rva": "198418", "text": "２列で６つの数字（ライン）に賭けます。"}, {"rva": "198440", "text": "この場合、１３,１４,１５,１６,１７,１８に賭けています。\n倍率は６倍です。"}, {"rva": "198490", "text": "４つの数字（コーナー）に賭けます。\nこの場合、１７,１８,２０,２１に賭けています。倍率は９倍です。"}, {"rva": "1984F4", "text": "縦１行（コラム）に賭けます。"}, {"rva": "198518", "text": "この場合、２,５,８,……,３２,３５に賭けています。倍率は３倍です。"}, {"rva": "198560", "text": "１ｓｔ１２、２ｎｄ１２、３ｒｄ１２にメダルを置くと、それぞれ\n１～１２、１３～２４、２５～３６に賭けることになります。\n倍率は３倍です。"}, {"rva": "1985E8", "text": "１－１８と、１９－３６は、それぞれ１～１８、１９～３６に賭けます。\n倍率は２倍です。"}, {"rva": "19863C", "text": "ＥＶＥＮは偶数のことです。偶数に賭けます。\n倍率は２倍です。"}, {"rva": "198678", "text": "ＯＤＤは奇数のことです。奇数に賭けます。\n倍率は２倍です。"}, {"rva": "1986B8", "text": "ＲＥＤとＢＬＡＣＫは数字ではなく、赤か黒か、色に賭けます。\n倍率は２倍です。"}, {"rva": "198704", "text": "当たる確率の低い賭け方ほど倍率はあがります。"}, {"rva": "198738", "text": "メダルは、１０枚ずつ、最大で１０ヶ所に置くことができます。\n色々な賭け方を試してください。"}, {"rva": "198794", "text": "メダルを置いて左上のスタートを選ぶと、\nルーレット開始です。"}, {"rva": "198DE8", "text": "◆━━━"}, {"rva": "198DF4", "text": "━━━◆"}, {"rva": "198E14", "text": "◆━━            ━━◆"}, {"rva": "198E3C", "text": "◆━━                   ━━◆"}, {"rva": "198E60", "text": "＝"}, {"rva": "199000", "text": "カジノへようこそ。\nここはブラックジャックで遊べます。%c"}, {"rva": "199060", "text": "◆━━━━"}, {"rva": "19906C", "text": "━━━━◆"}, {"rva": "1990D8", "text": " 始める"}, {"rva": "1990E0", "text": " 説　明"}, {"rva": "1990E8", "text": " 役説明"}, {"rva": "1990F0", "text": " やめる"}, {"rva": "1990F8", "text": " 続ける"}, {"rva": "199154", "text": "所持メダル"}, {"rva": "19916C", "text": "賭けメダル"}, {"rva": "199194", "text": "残りメダル"}, {"rva": "1991A8", "text": "このブラックジャックは\nプレイヤーとディーラーの２人で行います。"}, {"rva": "1991E8", "text": "使用するカードは全部で４×１３＝５２枚です。\nこのゲームではジョーカーは使用しません。"}, {"rva": "199240", "text": "まず、始めにプレイヤーとディーラーに\n２枚ずつカードが配られます。"}, {"rva": "199288", "text": "次にベット（賭けメダルの枚数決定）を行います。\n表になっているカードを参考に、\n１～１００枚の範囲内で好きな枚数を設定してください。"}, {"rva": "199310", "text": "※ブラックジャックでは、２～１０まではそのままの数字、\n　Ｊ，Ｑ，Ｋは１０、Ａは１か１１として数えます。\n　スート（マーク）はカードの強弱に関係しません。"}, {"rva": "1993B0", "text": "ベットが終わるとプレイヤーのターンになり、\nプレイヤーの裏になっていたカードがめくられます。"}, {"rva": "199410", "text": "この時点で手札の合計が２１になっている場合（Ｂ·Ｊ）、\nそのままディーラーのターンとなります。"}, {"rva": "199470", "text": "プレイヤーは自分の手札を見て\n以下の４つの行動を選択することができます。"}, {"rva": "1994B8", "text": "①『ＨＩＴ』\n　カードを１枚引く。\n　２１に到達するまで、何度でも引ける。"}, {"rva": "199504", "text": "②『ＳＴＡＮＤ』\n　カードを引かず、ディーラーのターンにする。"}, {"rva": "199548", "text": "③『ＤＯＵＢＬＥＤＯＷＮ』\n　カードを１枚引く。\n　１枚しか引けない代わりに、ベット額を２倍にする。"}, {"rva": "1995B0", "text": "④『ＳＵＲＲＥＮＤＥＲ』\n　互いの手札に関わらず必ずプレイヤーの負けとなるが、\n　ベット額の半分（切り捨て）が手元にもどる。"}, {"rva": "199630", "text": "『ＨＩＴ』あるいは『ＤＯＵＢＬＥＤＯＷＮ』を選び、\nプレイヤーの手札の合計値が２１を超えてしまうと、\nディーラーの手札に関係なくプレイヤーの負けとなります。"}, {"rva": "1996D0", "text": "なお、この場合は直後にディーラーが２１を超えたとしても、\nプレイヤーの負けとなります。"}, {"rva": "199728", "text": "『ＳＴＡＮＤ』を選ぶとディーラーのターンになります。"}, {"rva": "199760", "text": "ディーラーの裏になっていたカードが表になり、\nディーラーはＨＩＴあるいはＤＯＵＢＬＥＤＯＷＮをして\n手札の値を２１に近づけます。"}, {"rva": "1997E0", "text": "ディーラーのターンが終了すると\n互いの手札の合計値を比較して\nより２１に近いほうが勝利となります。"}, {"rva": "199848", "text": "同じ合計値だった場合、\n引き分け（ベット額分払い戻し）になります。\n（《役》ができている場合は『役説明』参照。）"}, {"rva": "1998B8", "text": "例外として、２１に到達する前に手札が６枚以上になると、\n無条件でプレイヤーの勝利となります。"}, {"rva": "199918", "text": "勝利した場合、その手札に応じて《役》の判定を行い、\n《役》のレート（倍率）に応じた配当メダルを受け取ります。"}, {"rva": "19A248", "text": "【ＢＬＡＣＫＪＡＣＫ】\nＡとＪ，Ｑ，Ｋ，１０のどれかの組み合わせ。\n（２枚で２１となる組み合わせ。）配当はベット額の３倍です"}, {"rva": "19A2C8", "text": "【ＱＵＥＥＮＪＡＣＫ】\nダイヤのＡとダイヤのＪの組み合わせ。\n配当はベット額の１０倍です。"}, {"rva": "19A328", "text": "【ＢＬＡＣＫＪＡＣＫ（純正）】\nスペードのＡとスペードのＪの組み合わせ。\n配当はベット額の５０倍です。"}, {"rva": "19A390", "text": "※プレイヤーとディーラーが共にＢＬＡＣＫＪＡＣＫの時、\n　Ｂ·Ｊ（純正）＞クイーンジャック＞Ｂ·Ｊ の順で\n優劣を決定します。全く同じ役の場合は引き分けです。"}, {"rva": "19A430", "text": "【７·７·７（スリーセブン）】\n手札が３枚で３枚とも全てが７。プレイヤーのみの役。\nディーラーの場合はただの２１扱い。配当はベット額の１００倍です。"}, {"rva": "19A4C8", "text": "【６ＣＡＲＤ】\n２１に到達する前に手札を６枚集める。プレイヤーのみの役。\n配当はベット額の１２０倍です。"}, {"rva": "19A530", "text": "【７ＣＡＲＤ】\n２１に到達する前に手札を７枚集める。プレイヤーのみの役。\n６ＣＡＲＤとは重複しない。配当はベット額の２５０倍です。"}, {"rva": "19A5B8", "text": "※７·７·７、６ＣＡＲＤ、７ＣＡＲＤは役ができれば\n　ディーラーの手札が何であれ、プレイヤーの勝利です。"}, {"rva": "19A620", "text": "なお、それ以外での勝利の場合、\n配当はベット額の２倍となります。"}, {"rva": "19AB38", "text": "メダルがありません。\n交換所でメダルとミラを交換してください。%c"}, {"rva": "19ABC4", "text": "ベットしてください。\n１～１００枚までベット出来ます。%c"}, {"rva": "19AC24", "text": "続けてプレイしますか？%c"}, {"rva": "19AF38", "text": "#11C%s#0Cが "}, {"rva": "19AF68", "text": "%c#2C#56Iセピス(地) x%d #0Cを吐き出した。"}, {"rva": "19AFB4", "text": "%c#2C#57Iセピス(水) x%d #0Cを吐き出した。"}, {"rva": "19B000", "text": "%c#2C#58Iセピス(火) x%d #0Cを吐き出した。"}, {"rva": "19B04C", "text": "%c#2C#59Iセピス(風) x%d #0Cを吐き出した。"}, {"rva": "19B098", "text": "%c#2C#62Iセピス(時) x%d #0Cを吐き出した。"}, {"rva": "19B0E4", "text": "%c#2C#60Iセピス(空) x%d #0Cを吐き出した。"}, {"rva": "19B130", "text": "%c#2C#61Iセピス(幻) x%d #0Cを吐き出した。"}, {"rva": "19B210", "text": "%c#2C\n#56Iセピス(地) x%d \n#57Iセピス(水) x%d \n#58Iセピス(火) x%d \n#59Iセピス(風) x%d \n#62Iセピス(時) x%d \n#60Iセピス(空) x%d \n#61Iセピス(幻) x%d \n#0Cを吐き出した。"}, {"rva": "19B2C8", "text": "%c#%dI#2C%s#0Cを、%d個吐き出した。"}, {"rva": "19B33C", "text": "竿を持っていません"}, {"rva": "19B364", "text": "エサを持っていません"}, {"rva": "19B3A0", "text": "どの竿を使いますか？"}, {"rva": "19B3D0", "text": "　　　竿　の　選　択　　　"}, {"rva": "19B410", "text": "どのエサをつけますか？"}, {"rva": "19B440", "text": "　　エ　サ　の　選　択　　"}, {"rva": "19B4A0", "text": "#5C%s#0C(#3C%d#0Cリジュ)が釣れた\n\n\n\n\n"}, {"rva": "19B4F0", "text": "釣れそうな気配がない"}, {"rva": "19B518", "text": "逃げられてしまった"}, {"rva": "19B544", "text": "◆━━　　　　　　　　　　　　━━◆"}, {"rva": "19B6F4", "text": "ようこそいらっしゃいました。\nここではポーカーが楽しめます。%c"}, {"rva": "19B764", "text": " は　い"}, {"rva": "19B76C", "text": " いいえ"}, {"rva": "19B804", "text": "Rate×"}, {"rva": "19B810", "text": "このポーカーはプレイヤーが１人で行います。\nディーラーはカードを配るだけで、\n直接ゲームに参加しません。"}, {"rva": "19B878", "text": "まず、始めにベット（賭けメダルの枚数決定）を行います。\n１～１０枚の範囲内で好きな枚数を設定してください。"}, {"rva": "19B8E8", "text": "ベットが終わると手元に５枚のカードが配られます。\nプレイヤーはこの手札を既定の《役》の形にすることで、\nレート（配当メダルの倍率）を得ることができます。"}, {"rva": "19B980", "text": "画面右下メニューの『ＣＨＡＮＧＥ』を選択すると、\n手元にあるカードを１度だけ交換することができます。\nこれは最大で５枚まで交換することができます。"}, {"rva": "19BA18", "text": "交換したくないカードは、そのカード自身を選択することで、\nホールド状態（交換しない状態）にすることができます。\n（もう１度選択することでホールド状態は解除できます。）"}, {"rva": "19BAC0", "text": "１枚も交換しない時は全ての手札をホールド状態にして、\n画面右下の『ＣＨＡＮＧＥ』を選択し、先に進めてください。"}, {"rva": "19BB30", "text": "なお、画面右下の『ＡＬＬＨＯＬＤ』を選択すると、\n全ての手札を同時にホールドすることができます。"}, {"rva": "19BB90", "text": "『ＣＨＡＮＧＥ』選択後は、手札から《役》を判定します。\n《役》の種類によってレートは変化し、\nレートに応じて、受け取るメダルの枚数が決定します。"}, {"rva": "19BC20", "text": "『ＣＨＡＮＧＥ』選択後、何らかの《役》ができ、\nレートが０になっていなければ、\nそのままプレイを続行するかどうかの選択肢が出てきます。"}, {"rva": "19BCA8", "text": "プレイを続行し、《役》を重ねていけば、\nレートはどんどん釣り上げていくことができます。"}, {"rva": "19BD00", "text": "続行を中止した場合は、賭けメダルの枚数と\nレートに応じた配当メダルを受け取り、最初に戻ります。"}, {"rva": "19BD60", "text": "レートが０になってしまった場合は、\n賭けメダルを失い、配当メダルももらえずに最初に戻ります。"}, {"rva": "19BEF4", "text": "～10."}, {"rva": "19C410", "text": "ノーペアです。\nあなたの負けです。"}, {"rva": "19C434", "text": "１ペアです。\n当然、続けますよね？"}, {"rva": "19C458", "text": "これは２ペアですね。"}, {"rva": "19C470", "text": "３オブアカインド。\nこれはいい手です。"}, {"rva": "19C498", "text": "お見事。\nストレートです。"}, {"rva": "19C4B4", "text": "フラッシュです。\nかなりの強運ですね。"}, {"rva": "19C4DC", "text": "やりますね。\nフルハウスです。"}, {"rva": "19C4FC", "text": "４オブアカインド。\n……かなりやりますね。"}, {"rva": "19C528", "text": "これは素晴らしい……\nストレートフラッシュです。"}, {"rva": "19C558", "text": "ロイヤルフラッシュ！\nあなたには参りました。"}, {"rva": "19C6D0", "text": "《役》の説明をします。\n《役》は全部で１０種類あります。"}, {"rva": "19C708", "text": "【１　ＰＡＩＲ】１ペア\n条件：同じ数字のカードが２枚ある。\nレートは２倍です。"}, {"rva": "19C758", "text": "【２　ＰＡＩＲ】２ペア\n条件：１ペアが２組ある。\nレートは３倍です。"}, {"rva": "19C7A0", "text": "【３　ＯＦ　Ａ　ＫＩＮＤ】３オブアカインド\n条件：同じ数字のカードが３枚ある。\nレートは４倍です。"}, {"rva": "19C808", "text": "【ＳＴＲＡＩＧＨＴ】ストレート\n条件：５枚のカードの数字が連続している。\nレートは８倍です。"}, {"rva": "19C868", "text": "※Ａのみ、Ｋから連続させることができます。\n　１０，Ｊ，Ｑ，Ｋ，Ａは可。\n　Ｊ，Ｑ，Ｋ，Ａ，２は不可。"}, {"rva": "19C8D0", "text": "【ＦＬＵＳＨ】フラッシュ\n条件：５枚のカード全てのスート（マーク）が同じ。\nレートは１０倍です。"}, {"rva": "19C930", "text": "【ＦＵＬＬＨＯＵＳＥ】フルハウス\n条件：３オブアカインドと１ペアが同時にできている。\nレートは１５倍です。"}, {"rva": "19C9A0", "text": "【４　ＯＦ　Ａ　ＫＩＮＤ】４オブアカインド\n条件：同じ数字のカードが４枚ある。\nレートは５０倍です。"}, {"rva": "19CA08", "text": "【ＳＴＲＡＩＧＨＴＦＬＵＳＨ】ストレートフラッシュ\n条件：ストレートとフラッシュが同時にできている。\nレートは１００倍です。"}, {"rva": "19CA88", "text": "【ＲＯＹＡＬＦＬＵＳＨ】ロイヤルフラッシュ\n条件：１０，Ｊ，Ｑ，Ｋ，Ａのストレートフラッシュ。\nレートは５００倍です。"}, {"rva": "19CB00", "text": "【ＮＯ　ＰＡＩＲ】ノーペア\n条件：前述した《役》に一つも当てはまらない。\nレートは０倍です。つまりこの手はあなたの敗北を意味します"}, {"rva": "19D03C", "text": "ベットしてください。\n１～１０枚までベットできます。%c"}, {"rva": "19D08C", "text": "ホールドするカードを\n選択してください。%c"}, {"rva": "19D2E0", "text": "？？？？"}, {"rva": "19D380", "text": "　　　　？？？　　　　"}, {"rva": "19D3B4", "text": "属性攻撃"}, {"rva": "19D3C0",